{
  "name": "simpl-bulk-manual-ui",
  "private": true,
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss -c tailwind.config.js -i src/styles.css -o ../static/manual-ui.css --minify",
    "build:js": "esbuild src/main.tsx --bundle --format=esm --minify --outfile=../static/manual-ui.js --loader:.ts=ts --loader:.tsx=tsx",
    "build": "npm run build:css && npm run build:js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-progress": "^1.1.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "esbuild": "^0.25.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  }
}
